{
  "env": {
    "browser": true,
    "es2021": true
  },
  "extends": ["google", "plugin:prettier/recommended"],
  "parserOptions": {
    "ecmaVersion": 12,
    "sourceType": "module"
  },
  "rules": {
    "require-jsdoc": "off",
    "linebreak-style": ["error", "windows"],
    "prettier/prettier": ["error", { "endOfLine": "auto" }]
  }
}
